{
  "name": "lumeo",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "./start-server.sh",
    "start:dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "vercel-build": "chmod +x vercel-build.sh && ./vercel-build.sh",
    "fly:deploy": "chmod +x fly-build.sh && ./fly-build.sh",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "server": "node server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\""
  },
  "homepage": "https://PaulAdutwum.github.io/Lumeo",
